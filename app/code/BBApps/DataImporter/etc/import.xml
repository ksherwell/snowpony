<?xml version="1.0"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_ImportExport:etc/import.xsd">
    <entity name="catalog_category" label="Categories" model="BBApps\DataImporter\Model\Import\Category" behaviorModel="BBApps\DataImporter\Model\Source\Import\Behavior\Basic" />
    <entity name="catalog_strike_price" label="Strike Price" model="BBApps\DataImporter\Model\Import\StrikePrice" behaviorModel="BBApps\DataImporter\Model\Source\Import\Behavior\Basic" />
    <entity name="catalog_product_stock" label="Product Stock Update"
