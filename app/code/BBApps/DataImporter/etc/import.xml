            behaviorModel="BBApps\DataImporter\Model\Source\Import\Behavior\Basic" />
    <entity name="attribute" label="Attribute" model="BBApps\DataImporter\Model\Import\Attribute" behaviorModel="BBApps\DataImporter\Model\Source\Import\Behavior\Basic" />
    <entity name="attribute_option" label="Attribute Options" model="BBApps\DataImporter\Model\Import\AttributeOption" behaviorModel="BBApps\DataImporter\Model\Source\Import\Behavior\AttributeOption" />
    <entity name="custom_customer_composite" label="Custom Customers and Addresses" model="BBApps\DataImporter\Model\Import\CustomerComposite" behaviorModel="Magento\ImportExport\Model\Source\Import\Behavior\Basic" />
