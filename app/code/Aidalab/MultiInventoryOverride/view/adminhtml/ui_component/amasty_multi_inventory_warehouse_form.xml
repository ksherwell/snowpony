<?xml version="1.0" encoding="UTF-8"?>

<form xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Ui:etc/ui_configuration.xsd">
    <fieldset name="general">
        <field name="wh_delivery_postcodes">
            <argument name="data" xsi:type="array">
                <item name="config" xsi:type="array">
                    <item name="dataType" xsi:type="string">text</item>
                    <item name="label" xsi:type="string" translate="true">Warehouse Delivery Postcodes</item>
                    <item name="formElement" xsi:type="string">input</item>
                    <item name="source" xsi:type="string">warehouse</item>
                    <item name="sortOrder" xsi:type="number">130</item>
                    <item name="dataScope" xsi:type="string">wh_delivery_postcodes</item>
                </item>
            </argument>
            <settings>
                <notice translate="true">
                    Enter values of Delivery Postcodes by comma separated.
                </notice>
            </settings>
        </field>
    </fieldset>
</form>
