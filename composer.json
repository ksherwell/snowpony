{
    "name": "magento/project-community-edition",
    "description": "eCommerce Platform for Growth (Community Edition)",
    "type": "project",
    "version": "2.2.4",
    "license": [
        "OSL-3.0",
        "AFL-3.0"
    ],
    "require": {
        "magento/product-community-edition": "2.2.4",
        "composer/composer": "@alpha",
        "mageplaza/magento-2-blog-extension": "^2.6",
        "trustpilot/module-reviews": "^1.0",
        "bronto/php-common-helper": "^1.0",
        "algolia/algoliasearch-magento-2": "^1.7",
        "algolia/algoliasearch-client-php": "^1.27",
        "bronto/magento-module-redemption": "^3.6",
        "bronto/magento-module-rating": "^3.6",
        "bronto/magento-module-product": "^3.6",
        "bronto/magento-module-optin": "^3.6",
        "bronto/magento-module-notification": "^3.6",
        "bronto/magento-module-magento": "^3.6",
        "bronto/magento-module-inventory": "^3.6",
        "bronto/magento-module-integration": "^3.6",
        "bronto/magento-module-email": "^3.6",
        "bronto/magento-module-coupon": "^3.6",
        "bronto/magento-module-contact": "^3.6",
        "bronto/magento-module-connector": "^3.6",
        "bronto/magento-module-cart": "^3.6",
        "bronto/magento-module-browse": "^3.6",
        "bronto/magento-module-all": "^3.6"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.2.0",
        "squizlabs/php_codesniffer": "3.2.2",
        "phpmd/phpmd": "@stable",
        "pdepend/pdepend": "2.5.2",
        "friendsofphp/php-cs-fixer": "~2.2.1",
        "lusitanian/oauth": "~0.8.10",
        "sebastian/phpcpd": "2.0.4"
    },
    "autoload": {
        "psr-4": {
            "Magento\\Framework\\": "lib/internal/Magento/Framework/",
            "Magento\\Setup\\": "setup/src/Magento/Setup/",
            "Magento\\": "app/code/Magento/"
        },
        "psr-0": {
            "": [
                "app/code/"
            ]
        },
        "files": [
            "app/etc/NonComposerComponentRegistration.php"
        ],
        "exclude-from-classmap": [
            "**/dev/**",
            "**/update/**",
            "**/Test/**"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Magento\\Sniffs\\": "dev/tests/static/framework/Magento/Sniffs/",
            "Magento\\Tools\\": "dev/tools/Magento/Tools/",
            "Magento\\Tools\\Sanity\\": "dev/build/publication/sanity/Magento/Tools/Sanity/",
            "Magento\\TestFramework\\Inspection\\": "dev/tests/static/framework/Magento/TestFramework/Inspection/",
            "Magento\\TestFramework\\Utility\\": "dev/tests/static/framework/Magento/TestFramework/Utility/"
        }
    },
    "minimum-stability": "stable",
    "repositories": [
        {
            "type": "composer",
            "url": "https://repo.magento.com/"
        },
        {
            "type": "composer",
            "url": "https://cdn.bronto.com/magento"
        }
    ],
    "extra": {
        "magento-force": "override"
    }
}
